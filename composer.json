{
  "name": "djinorm/djin",
  "type": "library",
  "description": "Djin ORM library",
  "keywords": [
    "djin",
    "orm"
  ],
  "authors": [
    {
      "name": "Timur Kasumov aka XAKEPEHOK",
      "role": "Main developer"
    }
  ],
  "license": "BSD-3-Clause",
  "require": {
    "php": ">=7.2.0",
    "ext-mbstring": "*",
    "ext-json": "*",
    "psr/container": "^1.0",
    "adbario/php-dot-notation": "^2.0"
  },
  "require-dev": {
    "sebastian/comparator": "^2.0",
    "php-di/php-di": "^5.4",
    "phpunit/phpunit": "^7.1"
  },
  "autoload": {
    "psr-4": {"DjinORM\\Djin\\": "src/"}
  },
  "autoload-dev": {
    "psr-4": {"DjinORM\\Djin\\": "tests/"}
  },
  "suggest": {
    "php-di/php-di": "Dependency injection container"
  }
}
